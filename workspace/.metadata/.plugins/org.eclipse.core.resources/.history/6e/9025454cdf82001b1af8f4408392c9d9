package quiz02;

public class Circle {

	//field
	private Coord center;
	private double radius;
	
	Circle(){}
	Circle(Coord center, double radius){
		this.center = center;
		this.radius = radius;
	}
	
	public void info() {
		System.out.print("중심좌표 ");
		center.info();
		System.out.println(", 반지름 "+radius+", 넓이 "+Math.PI*Math.pow(radius,2));
	}
}
